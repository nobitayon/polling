@page
@model MyClaimsModel

@{
    ViewData["Title"] = "My Claims";
}

<h1>My Claims</h1>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Type</th>
            <th>Value</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var claim in User.Claims)
        {
            <tr>
                <td>@claim.Type</td>
                <td>@claim.Value</td>
            </tr>
        }
    </tbody>
</table>
